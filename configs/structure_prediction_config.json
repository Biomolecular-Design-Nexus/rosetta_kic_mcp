{
  "_description": "Configuration for cyclic peptide structure prediction using Rosetta",
  "_source": "Extracted from examples/use_case_2_cyclic_peptide_structure_prediction.py",

  "nstruct": 10,
  "runtime": 900,
  "use_mpi": false,
  "num_processors": 1,

  "scoring": {
    "ex1": true,
    "ex2": true,
    "rama_cutoff": 3.0,
    "min_genkic_hbonds": 2,
    "min_final_hbonds": 2,
    "score_symmetric_gly_tables": true,
    "use_rama_filter": true
  },

  "sampling": {
    "closure_attempts": 250,
    "min_solution_count": 1,
    "cis_pro_frequency": 0.3,
    "output_fraction": 0.001,
    "compute_rmsd_to_lowest": true,
    "compute_ensemble_sasa_metrics": true
  },

  "mpi_settings": {
    "lambda": 1.25,
    "kbt": 0.62,
    "batchsize_by_level": 1,
    "auto_2level_distribution": true,
    "stop_after_time": true
  },

  "output": {
    "format": "silent",
    "include_energy_analysis": true,
    "include_logs": true,
    "mute_output": true,
    "no_color": true
  },

  "demo_mode": {
    "enabled": true,
    "max_demo_structures": 10,
    "energy_ranges": {
      "total_score": [-50.0, -20.0],
      "fa_atr": [-45.0, -25.0],
      "fa_rep": [2.0, 8.0],
      "fa_sol": [8.0, 15.0],
      "fa_elec": [-5.0, 2.0]
    },
    "processing_delay": 0.2
  }
}